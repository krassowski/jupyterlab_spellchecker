{
  "name": "@ijmbarr/jupyterlab_spellchecker",
  "version": "0.1.8",
  "description": "A spell checker for jupyterlab.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/ijmbarr/jupyterlab_spellchecker",
  "bugs": {
    "url": "https://github.com/ijmbarr/jupyterlab_spellchecker"
  },
  "license": "BSD-3-Clause",
  "author": "Iain Barr",
  "files": [
    "dictionaries/*{aff,dic}",
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/**/*.{json,}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ijmbarr/jupyterlab_spellchecker"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "jlpm run watch",
    "lint": "jlpm && jlpm run prettier && jlpm run eslint",
    "prettier": "prettier --write \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "prettier --list-different \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "lint:check": "jlpm run prettier:check && jlpm run eslint:check"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.2.1",
    "@jupyterlab/apputils": "2.2.2",
    "@jupyterlab/cells": "^2.2.2",
    "@jupyterlab/codemirror": "^2.2.1",
    "@jupyterlab/fileeditor": "^2.2.1",
    "@jupyterlab/notebook": "^2.2.2",
    "@jupyterlab/ui-components": "^2.2.1",
    "@jupyterlab/statusbar": "^2.2.0",
    "@lumino/widgets": "^1.13.3",
    "typo-js": "^1.0.3"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "^3.7.0",
    "@types/codemirror": "0.0.87",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^2.1.1"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
}
